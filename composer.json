{
    "name": "philiplb/crudlexsilex2",
    "type": "library",
    "description": "The Silex 2 implementation for CRUDlex ( https://github.com/philiplb/CRUDlex ).",
    "keywords": ["silex", "crud", "admin generator"],
    "homepage": "https://github.com/philiplb/CRUDlexSilex2",
    "license": "MIT",
    "authors": [
        {
            "name": "Philip Lehmann-BÃ¶hm",
            "email": "philip@philiplb.de"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/philiplb/sphpdox"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ptlis/MetaYaml"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "silex/silex": "^2.2, !=2.2.3",
        "philiplb/crudlex": "0.14.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.2",
        "symfony/browser-kit": "~3.3",
        "symfony/css-selector": "~3.3",
        "sphpdox/sphpdox": "dev-master",
        "eloquent/phony": "~3.0",
        "eloquent/phony-phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": { "CRUDlex\\": "src/CRUDlex" }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
